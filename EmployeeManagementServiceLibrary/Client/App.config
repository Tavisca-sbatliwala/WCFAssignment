        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IEmployeeAdd" />
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IEmployeeRemark" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/EmployeeManagementServiceLibrary/Service1/"
